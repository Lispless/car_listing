<h1>New Manufacturer Page</h1>

<%= form_for @manufacturer do |f| %>
  <p>
    <%= f.label :name, 'Manufacturer Name' %><br>
    <%= f.text_field :name %><br>
  </p>

  <p>
    <%= f.label :country, 'Manufacturer Country' %><br>
    <%= f.text_area :country %>
  </p>

  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
